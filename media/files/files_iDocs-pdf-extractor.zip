<script>
  try {


    document.getElementById("idtext").addEventListener("click", (e) => {
      // Get the text field
      let copyText = e.target;
      navigator.clipboard.writeText(copyText.innerHTML);
      // alert(`The Id:${copyText.innerHTML}is copied on clipboard`)
      let copied = document.getElementById("copied")
      copied.innerHTML = "Copied!"

      setTimeout(() => {
        copied.innerHTML = ""
      }, 1000);

    })
  } catch (error) {

  }

  try {
